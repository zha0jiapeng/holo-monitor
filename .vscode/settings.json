{
    "files.encoding": "utf8",
    "java.configuration.runtimes": [],
    "java.compile.nullAnalysis.mode": "automatic",
    "java.debug.settings.enableRunDebugCodeLens": true,
    "java.debug.settings.showToString": true,
    "java.debug.settings.showVariableTypesInVariablesView": true,
    "java.debug.settings.showQualifiedNames": false,
    "java.debug.settings.showHex": false,
    "java.debug.settings.showStaticVariables": false,
    "java.debug.settings.showLogicalStructure": true,
    "java.debug.settings.enableHotCodeReplace": true,
    "java.debug.settings.forceBuildBeforeLaunch": true,
    "java.debug.settings.vmArgs": "-Dfile.encoding=UTF-8 -Duser.language=zh -Duser.country=CN",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "args": ["-NoExit", "-Command", "chcp 65001"]
        },
        "Command Prompt": {
            "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"],
            "args": ["/k", "chcp 65001"]
        }
    },
    "files.autoGuessEncoding": true,
    "java.configuration.workspaceCacheLimit": 90,
    "maven.terminal.useJavaHome": true,
    "maven.executable.path": "",
    "java.import.maven.enabled": true,
    "java.maven.downloadSources": true,
    "java.saveActions.organizeImports": true,
    "editor.formatOnSave": false,
    "java.format.enabled": true,
    "java.format.settings.url": "",
    "java.format.settings.profile": "GoogleStyle",
    "java.configuration.updateBuildConfiguration": "automatic"
}